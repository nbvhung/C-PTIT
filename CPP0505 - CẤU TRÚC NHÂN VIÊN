#include<bits/stdc++.h>
using namespace std;
using ll =  long long;

struct NhanVien{
	string ma = "00001", ten, gt, ns, dc, mst, hd;
};

void nhap(NhanVien &a){
	getline(cin,a.ten);
	cin>>a.gt>>a.ns;
	cin.ignore();
	getline(cin,a.dc);
	cin>>a.mst>>a.hd;
}

void in(NhanVien a){
	cout<<a.ma<<" "<<a.ten<<" "<<a.gt<<" "<<a.ns<<" "<<a.dc<<" "<<a.mst<<" "<<a.hd;
}

int main(){
    struct NhanVien a;
    nhap(a);
    in(a);
    return 0;
}
