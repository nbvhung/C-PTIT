#include <bits/stdc++.h>
using namespace std;
using ll = long long;

ll gcd(ll a, ll b){
    if(b==0) return a;
    return gcd(b, a%b);
}

ll lcm(ll a, ll b){
    return a/gcd(a,b)*b;
}

int main(){
    int t; cin>>t;
    while(t--){
        ll n; cin>>n;
        ll res = 1;
        for(int i=1; i<=n; i++){
            res =lcm(i,res);
        }
        cout<<res<<endl;
    }
}
