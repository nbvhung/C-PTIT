#include <bits/stdc++.h>
using namespace std;
using ll = long long;


int main() {
    int t; cin>>t;
    cin.ignore();
    while(t--){
        string s; cin>>s;
        int n; cin>>n;
        set<char> se;
        for(char x : s){
            se.insert(x);
        }
        if(se.size() + n >= 26) cout<<1<<endl;
        else cout<<0<<endl;
    }
}
