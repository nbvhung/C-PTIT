#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main(){
    int t; cin>>t;
    while(t--){
        ll n; cin>>n;
        ll res = -1e9;
        for(int i=2; i<=sqrt(n); i++){
            while(n%i==0){
                res = i;
                n/=i;
            }
        }
        if(n!=1){
            res = n;
        }
        cout<<res<<endl;
    }
}
