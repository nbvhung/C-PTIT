#include <bits/stdc++.h>
using namespace std;
using ll = long long;

bool check(string s){
    if(s[s.size()-2]=='8' && s[s.size()-1]=='6') return true;
    return false;
}

int main(){
    int t; cin>>t;
    while(t--){
        string s; cin>>s;
        if(check(s)) cout<<"1\n";
        else cout<<"0\n";
    }
}
